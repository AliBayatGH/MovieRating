{
    "ConnectionStrings": {
        "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=MovieRatingDb;Trusted_Connection=True;MultipleActiveResultSets=true"
    },
    "Elasticsearch": {
        "Url": "http://localhost:9200"
    },
    "RateLimiting": {
        "TokenLimit": 100,
        "QueueLimit": 2,
        "ReplenishmentPeriodSeconds": 1,
        "TokensPerPeriod": 10
    },
    "Jwt": {
        "Key": "Your-Very-Long-And-Secure-Key-Here",
        "Issuer": "MovieRating",
        "Audience": "MovieRatingApi"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}